{
    "manifest_version": 3,
    "name": "Valorant Tracker - Tamii",
    "version": "1.0",
    "author": "github.com/Tamino1230",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "Tamii"
    },
    "icons": {
        "32": "icon/icon32.png",
        "64": "icon/icon64.png",
        "128": "icon/icon128.png"
    },
    "description": "Copies the import things in your clipboard if you open valoranttracker.",
    "permissions": [
        "clipboardWrite",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "https://tracker.gg/valorant/*"
    ]
}